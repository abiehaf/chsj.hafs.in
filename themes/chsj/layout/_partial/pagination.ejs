<nav id="pagination" role="navigation" class="hide-print">
  <% if (page.prev){ %>
    <a href="<%- config.root %><%- page.prev_link %>" class="btn prev" rel="prev"><%= __('prev') %></a>
  <% } else { %>
    <span class="btn prev"><%= __('prev') %></span>
  <% } %>
  <% if (page.next){ %>
    <a href="<%- config.root %><%- page.next_link %>" class="next" rel="next"><%= __('next') %></a>
  <% } else { %>
    <span class="btn next"><%= __('next') %></span>
    <% } %>
  <div class="clearfix"></div>
</nav>
